def sqrt(x):
    if x < 0:
        raise ValueError("Do not use negative number")
    result = x / 2
    for _ in range(25):
        result = (result + x / result) / 2
    return result

def factoriel(n):
    if n < 0:
        raise ValueError("The number is shouldn't be negative number")
    result = 1
    for i in range(2, n+1):
        result *= i
    return result

def power(x,y):
    if y < 0:
        x = 1 / x
        y = -y

    result = 1
    for _ in range(y):
        result *= x
    return result
